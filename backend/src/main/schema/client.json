{
  "id": "http://json-schema.org/geo",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "description": "A carpal client",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "string"
    },
    "driverGender": {
      "type": "string",
      "required": true
    },
    "carType": {
      "type": "string",
      "required": true
    },
    "hasMps": {
      "type": "boolean",
      "required": true
    },
    "locationHome": {
      "type": "object",
      "properties": {
        "latitude": {
          "type": "number"
        },
        "longitude": {
          "type": "number"
        },
        "placeName": {
          "type": "string"
        }
      }
    }
  }
}
