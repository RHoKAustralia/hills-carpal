{
  "id": "http://json-schema.org/geo",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "description": "A geographical coordinate",
  "type": "object",
  "properties": {
    "client": {
      "type": "string",
      "required": true
    },
    "pickupTimeAndDateInUTC": {
      "type": "date-time",
      "required": true
    },
    "locationFrom": {
      "type": "object",
      "properties": {
        "latitude": {
          "type": "number"
        },
        "longitude": {
          "type": "number"
        },
        "suburb": {
          "type": ["string", "null"],
          "optional": true,
          "required": false
        },
        "postcode": {
          "type": ["string", "null"],
          "optional": true,
          "required": false
        },
        "placeName": {
          "type": "string"
        }
      }
    },
    "locationTo": {
      "type": "object",
      "properties": {
        "latitude": {
          "type": "number"
        },
        "longitude": {
          "type": "number"
        },
        "suburb": {
          "type": ["string", "null"],
          "optional": true,
          "required": false
        },
        "postcode": {
          "type": ["string", "null"],
          "optional": true,
          "required": false
        },
        "placeName": {
          "type": "string"
        }
      }
    },
    "fbLink": {
      "type": "string",
      "required": true
    },
    "driverGender": {
      "type": "string",
      "required": true
    },
    "carType": {
      "type": "string",
      "required": true
    }
  }
}

